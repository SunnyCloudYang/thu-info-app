{
    "name": "thu-info-lib",
    "version": "0.1.2-alpha.20",
    "description": "The JavaScript library that provides a program-friendly interface to Tsinghua web portal",
    "main": "lib/index.js",
    "files": [
        "lib/**/*",
        "src/**/*"
    ],
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
        "fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
        "draft": "node versionhelper.js draft",
        "release": "node versionhelper.js release",
        "build-dist": "webpack --config webpack.config.js",
        "watch-dist": "webpack -w --config webpack.config.js"
    },
    "keywords": [
        "tsinghua",
        "thu",
        "info",
        "thuinfo",
        "thu-info",
        "thu info"
    ],
    "author": "UNIDY2002 <UNIDY2002@outlook.com> (https://unidy2002.github.io/)",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/UNIDY2002/thu-info-lib.git"
    },
    "dependencies": {
        "abort-controller": "3.0.0",
        "cheerio": "0.22.0",
        "convert-excel-to-json": "1.7.0",
        "cross-fetch": "3.0.6",
        "dayjs": "1.10.4",
        "iconv-lite": "0.6.2",
        "md5": "2.3.0"
    },
    "devDependencies": {
        "@types/cheerio": "0.22.23",
        "@types/convert-excel-to-json": "1.7.0",
        "@types/jest": "26.0.20",
        "@types/md5": "2.3.0",
        "@typescript-eslint/eslint-plugin": "4.15.0",
        "@typescript-eslint/parser": "4.15.0",
        "eslint": "7.19.0",
        "html-webpack-plugin": "5.0.0",
        "jest": "26.6.3",
        "node-polyfill-webpack-plugin": "1.0.2",
        "semver": "7.3.4",
        "ts-jest": "26.5.0",
        "ts-loader": "8.0.16",
        "typescript": "4.1.3",
        "webpack": "5.21.2",
        "webpack-cli": "4.5.0"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node"
    }
}
